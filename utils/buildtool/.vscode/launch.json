	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Запустить",
			"type": "cppdbg",
			"request": "launch",
			"program": "${command:makefile.launchTargetPath}",
			"args": ["readelf", "/home/sbkarr/stappler/dev/stappler-musl-toolchain/toolchain-linux/fdhelper/x86_64/fdhelper"],
			"stopAtEntry": false,
			"cwd": "${command:makefile.getLaunchTargetDirectory}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Включить автоматическое форматирование для gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Задать для варианта приложения дизассемблирования значение Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	]
}